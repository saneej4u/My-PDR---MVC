@model NewPDR.Web.ViewModel.TitleViewModel
<div style="padding:10px 10px 10px 20px;" class="dynamicTile">
    <div class="row ">
        @if (Model.AllUsers != null)
        {
            foreach (var colPdr in Model.AllUsers)
            {
                <div class="col-sm-4 col-xs-8">
                    <div id="tile17" class="tile">
                        <div class="row">
                            <div class="col-md-4 col-sm-4 col-xs-4 text-center" style="">
                                <i class="fa fa-user fa-5x"></i>
                            </div>
                            <div class="col-md-8 col-sm-8 col-xs-8">
                                <address>
                                    <strong>Email :@colPdr.UserName</strong> <br />
                                    From: @colPdr.UserType.Name <br />
                                    @Html.ActionLink("View more...", "ProfileDetails", new { emailId = @colPdr.UserName })
                                </address>

                            </div>

                        </div>

                    </div>
                </div>
            }
        }
    </div>
</div>
